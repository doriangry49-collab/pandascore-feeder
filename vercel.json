{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/teams",
      "dest": "api/teams.py"
    },
    {
      "src": "/api",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api",
      "schedule": "0 0 * * *"
    }
  ]
}
