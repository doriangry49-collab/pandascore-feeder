{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "api/predict.py",
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/analyze",
      "dest": "api/analyze.py",
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api/teams",
      "dest": "api/teams.py",
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    },
    {
      "src": "/api",
      "dest": "api/index.py",
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      }
    }
  ],
  "crons": [
    {
      "path": "/api",
      "schedule": "0 0 * * *"
    }
  ],
  "public": true
}
